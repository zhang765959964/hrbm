	<!-- 查询个人工作情况-->
	<select id="getPersonWorkExpceByPersonId" resultType="java.util.HashMap">
		select  c.EXP_START_JOB_DATE as "expStartJobDate",c.EXP_OLD_COMPANY as "expOldCompany",c.EXP_POST as "expPost",c.EXP_ZHUANYE as "expZhuanye"
				,a.AAC0B0 as "aac0b0",a.AAC045 as "aac045"
				,a.AAC015 as "aac015",a.AAE013 as "aae013"
				,a.AAC0B0 as "aac0b0"
						
		from ac01 b
		left join RSDA_BS_PERSON_EXPAND c on c.EXP_PERSON_ID=b.AAC001
		left join (SELECT * FROM ( select t.*,ROWNUM RN from AC0B t where t.AAC001=#{AAC001} and t.CCPR08=#{CCPR08} order by t.AAE030  desc,t.AAE031 desc) WHERE ROWNUM=1) a
			  on b.AAC001=a.AAC001
       
       where b.AAC001=#{AAC001} 
	</select>
	
