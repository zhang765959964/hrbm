	<select id="getpresonInfoMap" resultType="java.util.Map">
	 select 
	         a.aac001,
	         floor(MONTHS_BETWEEN(sysdate, a.AAC006)/12) as AAC006, 
		     a.aac180,
		     a.aac003,
		     a.aac034,
		     A.BAB305,
		     to_char(a.aac181,'yyyy-MM-dd') aac181, 
		     a.aac025,
		     to_char( a.CCPR05, 'yyyy-MM-dd hh24:mi:ss') CCPR05,
		     a.aac004,
		     edu.name as eduName,
		     nation.name as nationName,
		     workyears.name as workyearsName,
		     affil.name as affilName,
		     b.name as marriageStatus,
		     c.ccmu15,
		     sex.name as sexName
			FROM AC01 a
			left join TAB_MARRIAGE b on a.aac017=b.code
		    left join ccm01 c  on a.ccmu01=c.ccmu01
	        left join TAB_EDUCATION edu on a.AAC011=edu.code
	        left join TAB_NATION nation on a.AAC005=nation.code
	        left join tab_workyears  workyears on a.AAC038=workyears.code
	        left join TAB_AFFIL  affil on a.AAC024=affil.code
