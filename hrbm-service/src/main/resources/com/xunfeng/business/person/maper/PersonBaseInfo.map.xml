		 <!-- 	AAC001,AAC002,AAC00A,AAC003,AAC004,AAC005,AAC006,AAC009,AAB301,AAC00B,AAC00C,AAC00D,BAB305,AAE005,AAC00E,AAC011,AAC180,AAC181,AAC017,AAC024,AAC033,AAE015,AAC022,AAE007,AAC023,AAC032,AAC034,AAC035,AAC036,AAC025,AAE013,AAE011,AAE017,AAE036,AAE019,AAE020,AAE022,CCMU01,CCMU51,CCMU52,CCMU53,CCMU54,CCMU55,CCMU56,CCZY06,CCMU57,CCMU58,CCMU59,CCMU60,CCMU61,CCMU62,CCPR08,CCMU63,ACC01G,AAC037,AAC038 -->
		  SELECT AAE020,ECZY06,COUNT(1) AS PERNUM  
		  <choose>
		  <when test="@Ognl@isNotEmpty(groupname)">,${columnname} as columnname</when>
		  <otherwise> ,'' AS COLUMNNAME </otherwise>
		  </choose>
		  FROM 
		  V_AC01 
		  WHERE 1=1
		  <include refid="statisticsWhere" />
		  GROUP BY AAE020,ECZY06
		  <if test="@Ognl@isNotEmpty(groupname)">,${groupname}</if>
		</select>	
		<select id="getPersonStatisticsCount" resultType="java.util.HashMap">
		 <!-- 	AAC001,AAC002,AAC00A,AAC003,AAC004,AAC005,AAC006,AAC009,AAB301,AAC00B,AAC00C,AAC00D,BAB305,AAE005,AAC00E,AAC011,AAC180,AAC181,AAC017,AAC024,AAC033,AAE015,AAC022,AAE007,AAC023,AAC032,AAC034,AAC035,AAC036,AAC025,AAE013,AAE011,AAE017,AAE036,AAE019,AAE020,AAE022,CCMU01,CCMU51,CCMU52,CCMU53,CCMU54,CCMU55,CCMU56,CCZY06,CCMU57,CCMU58,CCMU59,CCMU60,CCMU61,CCMU62,CCPR08,CCMU63,ACC01G,AAC037,AAC038 -->
		  SELECT COUNT(1) AS TOTALNUM 
		  FROM 
		  V_AC01 
		  WHERE 1=1
	  		<include refid="statisticsWhere" />
	</select>
	<sql id="statisticsWhere">
		  <choose>
			<when test="@Ognl@isNotEmpty(view_sub)">
			 <if test="@Ognl@isNotEmpty(curr_org)">
			  AND AAE017 IN ( SELECT  ACD200  FROM CD20 M START WITH M.ACD200= #{curr_org} CONNECT BY M.AAB034=PRIOR M.ACD200 )
			 </if>
			</when>
			<otherwise>
			 <if test="@Ognl@isNotEmpty(curr_org)">
			  AND AAE017 = #{curr_org}</if>
			</otherwise>
	  	  </choose>
	      <if test="@Ognl@isNotEmpty(aae017)"> AND AAE017 = #{aae017}</if>
		  <if test="@Ognl@isNotEmpty(cczy06)"> AND CCZY06 = #{cczy06}</if>
		  <if test="@Ognl@isNotEmpty(startdate)"> AND AAE036 >= #{startdate}</if>
	  	  <if test="@Ognl@isNotEmpty(enddate)"> AND AAE036 <![CDATA[<= #{enddate}]]></if></sql>
	<select id="getPersonStatisticsEcharts" resultType="java.util.HashMap">
		  SELECT  COUNT(1) AS PERNUM  
		 	,${columnname} as COLUMNNAME
		  FROM 
		  V_AC01 
		  WHERE 1=1
		  <include refid="statisticsWhere" />
		  GROUP BY  ${groupname} 
		  ORDER BY  ${groupname} 
	</select>
	<!-- 查询个人基本信息 -->
	<select id="getReceivePersonByPersonId" resultType="java.util.HashMap">
		select  p.aac001 as "aac001",i.EXPAND_ID as "expandId",a.id as "id",p.CCMU61 as "ccmu61",p.AAC004 as "aac004",
				p.AAC002 as "aac002",p.AAC003 as "aac003",to_char(p.AAC006,'yyyy-mm-dd') as "aac006"
				,i.EXP_USER_NAME as "expUserName",e.name as "genderName"
				,a.ARCHIVE_NAME as "archiveName",p.AAC005 as "aac005",p.AAC024 as "aac024",p.AAC011 as "aac011"
				,i.EXP_PERSON_PHONE as "expPersonPhone",p.AAE005 as "aae005"
				,i.EXP_IS_CADRE as "expIsCadre",i.EXP_IS_DISCOUNT as "expIsDiscount",i.EXP_REGISGRATION as "expRegisgration",i.EXP_IS_REGISGRATION as "expIsRegisgration"
				,i.EXP_CARD_NUMBER as "expCardNumber",p.ACC0M1 as "acc0m1",p.BAB301 as "bab301",p.AAB301 as "aab301"
				,p.AAE013 as "aae013"
				,p.ACC01G as "acc01g",i.EXP_POSITION_CODE as "expPositionCode"
				,a.ARCHIVE_NAME as "archiveName"
				,i.EXP_SOCTAL_NUMBER as "expSoctalNumber",i.EXP_PENSION_DATE as "expPensionDate",i.EXP_MEDICAL_DATE as "expMedicalDate",i.EXP_INSURANCE as "expInsurance"
				,i.EXP_ARREARS as "expArrears",i.EXP_BORROWING_STATUS as "expBorrowingStatus",i.EXP_RETIRE as "expRetire"
				,i.EXP_SOCIALIZATION as "expSocialization",i.EXP_ORGANIZE as "expOrganize",i.EXP_RESIDENCE as "expResidence"
				,i.EXP_PENSION as "expPension",i.EXP_MEDICAL as "expMedical",i.EXP_INJURY as "expInjury",i.EXP_BIRTH as "expBirth"
				,i.EXP_SERIOUS as "expSerious",i.EXP_UNEMPLOYMENT as "expUnemployment",i.EXP_ACCUMULATION as "expAccumulation"
					
						
		from ac01 p
		left join RSDA_BS_PERSON_EXPAND i on i.EXP_PERSON_ID=p.AAC001
		left join (select * from RSDA_BS_ARCHIVE_ARCHIVES b where b.data_sources = 1 ) a on a.PERSON_ID=p.AAC001
        left join TAB_SEX e on e.code=p.AAC004
       
       where p.aac001=#{personId}
	</select>
	<select id="getMapPersonList" resultType="java.util.HashMap">
		select t.aac001 as "aac001",t.aac002 as "aac002",t.aac003 as "aac003",t.aac004 as "aac004",t.expCardNumber as "expCardNumber" from V_AC01 t
			 where upper(t.AAC002)  LIKE upper(#{searchValue})   or t.AAC003  LIKE #{searchValue} 
	</select>
</mapper>
