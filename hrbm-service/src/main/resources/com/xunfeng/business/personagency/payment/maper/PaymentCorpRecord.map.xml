	<select id="getOverUnitAgreement" resultMap="unitAgreementDto">
		select a.*,decode(a.ua_status,0,'有效',1,'无效') ua_statuss,AAB001,AAB004,AAB007,AAB019,AAB020,AAE006,AAB301,AAA021,AAE004,AAE005,BAB002,u.name as unitname,e.name as econname
			  from rsda_bs_unit_agreement a
			  left join ab01 b on b.aab001 = a.ua_unit_id
			  left join ZJ.TAB_UNITNATURE u ON  b.AAB019 = u.CODE
              left join ZJ.TAB_ECONOMIC e ON  b.aab020 = e.CODE
			  where a.ua_unit_id in (select t.corp_id
			                          from V_RSDA_PERSON_PAYMENT_MAIN t
                         where t.fd_end_payment_date <![CDATA[ < #{endPaymentDate}]]>
                           and t.corp_id is not null
                         group by t.corp_id)
            <if test="@Ognl@isNotEmpty(ids)"> AND a.Id  in (${ids}) </if>              
