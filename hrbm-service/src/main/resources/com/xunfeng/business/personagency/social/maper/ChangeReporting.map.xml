	<select id="getAll" resultMap="ChangeReportingDto">
		SELECT <include refid="columns"/>
				,e.name as "genderName"
				,f.name as "nationName"
				,b.name as "employmentFormName"
				,c.name as "changeContentName" 
				,d.name as "reasonsForStoppingName"
				,decode(a.ADD_IS_INSURANCE , 0, '否', 1, '是') as "addIsInsuranceName"
				,g.name as "pensionBaseName"
				,h.name as "medicalBaseName" 
				,decode(a.IS_EFFECTIVE , 0, '有效', 1, '无效') as "isEffectiveName"
				
		FROM RSDA_BS_SOCIALSEC_PERSON_LOG a
		left join TAB_SEX e on e.code=a.AAC004
		left join TAB_NATION f on f.code=a.AAC005
		left join RSDA_TAB_EMPLOYMENT_FORM b on b.code=a.EMPLOYMENT_FORM
		left join RSDA_TAB_CHANGE_CONTENT c on c.code=a.CHANGE_CONTENT
		left join RSDA_tab_STOPSOCIAL_REASON d on d.code=a.REASONS_FOR_STOPPING
		left join RSDA_TAB_PENSIONBASE g on g.code=a.PENSION_BASE
		left join RSDA_TAB_MEDICALBASE h on h.code=a.MEDICAL_BASE
		  
		<include refid="dynamicWhere" />
		<if test="@Ognl@isNotEmpty(orderField)">
		order by ${orderField} ${orderSeq}
		</if>
		<if test="@Ognl@isEmpty(orderField)">
		order by a.ID  desc
		</if>
	</select>
	
