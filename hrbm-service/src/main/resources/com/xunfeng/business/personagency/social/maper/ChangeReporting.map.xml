	<select id="getChangeReportingById" resultType="java.util.HashMap">
		SELECT 	a.ID as "id",a.AAC002 as "aac002",a.AAC003 as "aac003"
				,to_char(a.AAC006,'yyyymm') as "aac006",a.EXP_PERSON_NUMBER as "expPersonNumber"
				,f.name as "nationName",e.name as "genderName",to_char(a.EXP_START_JOB_DATE,'yyyymm') as "expStartJobDate"
				,d.name as "employmentFormName",a.AAE005 as "aae005",a.AAB301 as "aab301",a.AAE007 as "aae007"
				,a.CHANGE_CONTENT as "changeContent",a.ADD_IS_INSURANCE as "addIsInsurance",a.CHECKOUT_SOCIAL_ORG_NAME as "checkoutSocialOrgName"
				,to_char(a.FIELD_START_DATE,'yyyy') as "startYear",to_char(a.FIELD_START_DATE,'mm') as "startMonth"
				,to_char(a.FIELD_END_DATE,'yyyy') as "endYear",to_char(a.FIELD_END_DATE,'mm') as "endMonth"
				,to_char(a.EXP_PENSION_DATE,'yyyy') as "personYear",to_char(a.EXP_PENSION_DATE,'mm') as "personMonth"
				,to_char(a.EXP_MEDICAL_DATE,'yyyy') as "medicalYear",to_char(a.EXP_MEDICAL_DATE,'mm') as "medicalMonth"
				,to_char(a.STOP_SOCIAL_DATE,'yyyy') as "stopYear",to_char(a.STOP_SOCIAL_DATE,'mm') as "stopMonth"
				,h.name as "personBaseName",a.MEDICAL_BASE as "medicalBase",a.CREATER_NAME as "createrName"
				,to_char(a.CREATER_DATE,'yyyy') as "creatYear",to_char(a.CREATER_DATE,'mm') as "creatMonth",to_char(a.CREATER_DATE,'dd') as "creatDay"
		
		FROM RSDA_BS_SOCIALSEC_PERSON_LOG a
		left join ac01 p on p.AAC001=a.PERSON_ID
		left join TAB_SEX e on e.code=a.AAC004
        left join TAB_NATION f on f.code=a.AAC005
        left join RSDA_TAB_EMPLOYMENT_FORM d on d.code=a.EMPLOYMENT_FORM
        left join RSDA_TAB_PENSIONBASE h on h.code=a.PENSION_BASE
        
		where a.ccpr08=0 and a.id =#{id}
